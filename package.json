{
  "name": "obpr-sato-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x",
    "pnpm": "9.x",
    "yarn": "Please use pnpm!",
    "npm": "Please use pnpm!"
  },
  "config": {},
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "lefthook install",
    "fix": "pnpm run '/.*:fix$/'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:cache": "eslint 'src/**/*.{js,jsx,ts,tsx}' --cache --cache-strategy=content",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "fmt": "prettier --check 'src/**/*.{js,jsx,ts,tsx,html,json}'",
    "fmt:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,html,json}'",
    "typecheck": "tsc",
    "spellcheck": "cspell 'src/**/*.{js,jsx,ts,tsx,html,json}' --no-progress",
    "preinstall": "npx typesync || :",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:typecheck": "vitest --typecheck.only",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "scaffold:fc": "hygen fc new",
    "scaffold:hooks": "hygen hooks new",
    "scaffold:provider": "hygen provider new",
    "find-deadcode": "pnpm run '/find-deadcode:.*/'",
    "find-deadcode:files": "knip --include files",
    "find-deadcode:types": "knip --include types",
    "find-deadcode:exports": "knip --include exports",
    "reset": "scripts/reset.sh",
    "pre-push": "scripts/pre-push.sh",
    "prevent-main": "scripts/protect-default-branch.sh"
  },
  "dependencies": {
    "@apollo/client": "^3.11.1",
    "@ebay/nice-modal-react": "^1.2.13",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.13",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/lab": "5.0.0-alpha.172",
    "@mui/material": "^5.16.4",
    "@mui/x-date-pickers": "^7.11.0",
    "@sentry/react": "^8.19.0",
    "@tanstack/react-query": "^5.51.11",
    "@tanstack/react-query-devtools": "^5.51.11",
    "date-fns": "^3.6.0",
    "encoding-japanese": "^2.2.0",
    "graphql": "^16.9.0",
    "lodash-es": "^4.17.21",
    "notistack": "^3.0.1",
    "papaparse": "^5.4.1",
    "query-string": "^9.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.52.1",
    "react-router-dom": "^6.25.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@hookform/devtools": "^4.3.1",
    "@storybook/addon-actions": "8.1.11",
    "@storybook/addon-essentials": "8.1.11",
    "@storybook/addon-interactions": "8.1.11",
    "@storybook/addon-links": "8.1.11",
    "@storybook/addon-mdx-gfm": "8.1.11",
    "@storybook/blocks": "8.1.11",
    "@storybook/react": "8.1.11",
    "@storybook/react-vite": "8.1.11",
    "@storybook/test": "8.1.11",
    "@testing-library/react": "^16.0.0",
    "@types/encoding-japanese": "^2.2.0",
    "@types/eslint": "^8.56.10",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.11",
    "@types/papaparse": "^5.3.14",
    "@types/prop-types": "^15.7.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "cspell": "^8.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.1",
    "hygen": "^6.2.11",
    "knip": "^5.26.0",
    "lefthook": "^1.7.6",
    "mockdate": "^3.0.5",
    "msw": "^2.3.2",
    "msw-storybook-addon": "^2.0.3",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "storybook": "8.1.11",
    "storybook-addon-remix-react-router": "^3.0.0",
    "type-fest": "^4.22.1",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vite-plugin-inspect": "^0.8.5",
    "vite-plugin-sentry": "^1.4.0",
    "vite-plugin-turbosnap": "^1.0.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
